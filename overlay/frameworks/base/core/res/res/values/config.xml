<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Whether a software navigation bar should be shown. NOTE: in the future this may be
         autodetected from the Configuration. -->
    <bool name="config_showNavigationBar">true</bool>
    
    <!-- Whether PUBG bast kramel mode FPS ultra mode governor should
         be enabled by default -->
    <bool name="config_enableBastKramelPUBGMode">true</bool>

      <!-- turn phone to a Vibrator in feedback about a long screen/key press -->
    <integer-array name="config_longPressVibePattern">
        <item>69</item>
        <item>420</item>
        <item>6969</item>
    </integer-array>

    <!-- turn phone to a Vibrator on touching a virtual key -->
    <integer-array name="config_virtualKeyVibePattern">
        <item>69</item>
        <item>420</item>
    </integer-array>

    <!-- turn phone to a Vibrator for soft keyboard tap -->
    <integer-array name="config_keyboardTapVibePattern">
        <item>69</item>
    </integer-array>

    <integer name="config_defaultRingVibrationIntensity">6969</integer>
    <integer name="config_defaultNotificationVibrationIntensity">420</integer>
    <integer name="config_defaultHapticFeedbackIntensity">69</integer>
    
